<%= form_for warehouse do |f| %>
    <%= f.label :name, 'Nome' %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :code, 'Código' %>
    <%= f.text_field :code %>
    <br>
    <%= f.label :city, 'Cidade' %>
    <%= f.text_field :city %>
    <br>
    <%= f.label :area, 'Área' %>
    <%= f.number_field :area %>
    <br>
    <%= f.label :address, 'Endereço' %>
    <%= f.text_field :address %>
    <br>
    <%= f.label :zip, 'CEP' %>
    <%= f.number_field :zip %>
    <br>
    <%= f.label :description, 'Descrição' %>
    <%= f.text_field :description %>
    <br>
    <%= f.submit 'Salvar'%>
<%end%>