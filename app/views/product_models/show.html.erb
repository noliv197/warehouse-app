
<%#= link_to 'Editar', @product_model%>

<h1><%=@product_model.name%></h1>
<dl>
    <dt>Informações Gerais</dt>
    <dd>Código:<%=@product_model.sku%></dd>
    <dd>Dimensões: <%=@product_model.height%>cm x <%=@product_model.width%>cm x <%=@product_model.depth%>cm</dd>
    <dd>Peso: <%=@product_model.weight%></dd>
    <dd>Fornecedor: <%=@product_model.supplier.corporate_name%></dd>
</dl>